 "css": ".main-div{\n    border: 1px solid;\n    font-size: 11px;\n}\n.main-div .header{\n    text-align: center;\n    border-bottom: 1px solid;\n}\n.header h1{\n    font-size: 35px;\n    font-weight: 600;\n    margin-top: -1px;\n    margin-bottom: -1px;\n}\n.main-div .body-1{\n    width: 100%;\n    border-bottom: 1px solid;\n    margin-left: 1px;\n}\n.body-1 .e-invoice{\n    display: flex;\n    flex-direction: column;\n    align-items: end;\n    margin-top: -40px;\n}\n.e-invoice b{\n    font-size: 18px;\n}\nth,td{\n    /*padding: 3px;*/\n    font-size: 11px;\n}\n.body-2 .seller{\n    border-right: 1px solid;\n    margin-top: -16px;\n}\n.body-2 .buyer{\n    margin-left: -10px;\n    margin-top: -7px;\n}\n.body-3{\n    border-bottom: 1px solid black;\n}\n.body-3 table{\n    width: 100%;\n}\n.body-3 th{\n    border: 1px solid;\n    text-align: center;\n    border-collapse: collapse;\n    background-color: rgb(234, 228, 228);\n    font-weight: bold;\n    color: black;\n}\n.body-3 td{\n    border-right: 1px solid;\n}\n.body-3 .sno{\n    font-size: 7px;\n}\n.body-3 .tax{\n    text-align: end;\n    margin-top: -3px;\n    border-bottom: 1px double;\n    display: flex;\n    width: 100%;\n}\n.body-3 .total{\n    text-align: end;\n    display: flex;\n    border-top: 1px solid;\n    border-bottom: 1px solid white;\n}\n.body-3 .total b{\n    font-size: 16px;\n}\n.body-4{\n    border-bottom: 1px solid;\n    \n}\n.body-5{\n    display: flex;\n    height: 10vh;\n}\n.body-5 .sign{\n    width: 30%;\n    border-right: 1px solid;\n    display: flex;\n    justify-content: center;\n    align-items: end;\n}\n.body-5 .auth-sign{\n    width: 40%;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    align-items: end;\n}",
